<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.salesappkotlinproject">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".data.local.ApplicationEnactus"
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.authorization.AuthorizationActivity"></activity>
        <activity android:name=".ui.authorization.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.salesappkotlinproject.ui.owner.edit_product.EditProductActivity" />
        <activity android:name="com.example.salesappkotlinproject.ui.owner.sell_product.SellProductActivity" />
        <activity android:name="com.example.salesappkotlinproject.ui.owner.detail_product.DetailProductActivity" />
        <activity android:name="com.example.salesappkotlinproject.ui.owner.main.MainActivity" />
    </application>

</manifest>